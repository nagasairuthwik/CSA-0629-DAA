#include<stdio.h>

int main()

{

	int n,a[50],i,x;

	printf("enter size of array");

	scanf("%d",&n);

	printf("enter elements\n");

	for(i=0;i<n;i++)

	{

		scanf("%d",&a[i]);

	

	}

	printf("enter search element");

	scanf("%d",&x);

	for(i=0;i<n;i++)

	{

		if(a[i]==x)

		printf("%d element found at %d ",x,i+1);

	}



	return 0;

}

